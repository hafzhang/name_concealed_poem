{
  "name": "BeautifierExtension",
  "displayName": "Beautifier Extension",
  "description": "Auto-beautify mounting styles and run long pipelines",
  "version": "0.0.1",
  "activationEvents": ["onStartup", "onCommand:trae.beautifier.start", "onCommand:trae.pipeline.start"],
  "main": "./extensions/editor-helper.js",
  "contributes": {
    "commands": [
      {
        "command": "trae.beautifier.start",
        "title": "Beautifier: Start long-running"
      },
      {
        "command": "trae.pipeline.start",
        "title": "Pipeline: Start long-running"
      }
    ]
  }
}
